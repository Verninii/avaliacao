{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "/task-management/dist/$1"
    }
  ],
  "builds": [
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "task-management/dist/**/*",
      "use": "@vercel/static"
    }
  ]
}
